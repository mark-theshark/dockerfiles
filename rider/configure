# Install .net framework SDK
sudo /coder/dotnet-install.sh
sudo ln -s ~/.dotnet/dotnet /usr/bin/dotnet

# install  PowerShell VS Code extension into code-server
SERVICE_URL=https://open-vsx.org/vscode/gallery ITEM_URL=https://open-vsx.org/vscode/item /var/tmp/coder/code-server/bin/code-server --install-extension ms-vscode.powershell

# install  C# VS Code extension into code-server
SERVICE_URL=https://open-vsx.org/vscode/gallery ITEM_URL=https://open-vsx.org/vscode/item /var/tmp/coder/code-server/bin/code-server --install-extension muhammad-sammy.csharp

# install  F# VS Code extension into code-server
SERVICE_URL=https://open-vsx.org/vscode/gallery ITEM_URL=https://open-vsx.org/vscode/item /var/tmp/coder/code-server/bin/code-server --install-extension Ionide.Ionide-fsharp